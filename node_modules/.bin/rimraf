#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@ember-data/private-build-infra/node_modules/broccoli-plugin/node_modules/rimraf/bin.js" "$@"
  ret=$?
else 
  node  "$basedir/../@ember-data/private-build-infra/node_modules/broccoli-plugin/node_modules/rimraf/bin.js" "$@"
  ret=$?
fi
exit $ret
